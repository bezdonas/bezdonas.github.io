/* Media queries. 
Fallbacks by respond.js in plugins.js file. 
There is no much media code, so this way will not slow old browsers down */

@media screen and (max-width: 1199px) {
	.blogs-news ._optional {
		display: block;
	}

	.content_sidebar {
		/*display: none;*/
		position: absolute;
		left: 300%;
		-webkit-animation: sideVanish .4s linear;
		animation: sideVanish .4s linear;
	}

	.content_sidebar .anons-list {
		opacity: 0;
	}

	.blogs-news .tab-links ._optional a {
		-webkit-animation: blinkBlackGrey 1s .4s;
		animation: blinkBlackGrey 1s .4s;
	}

	.content_main {
		width: 100%;
	}

	.newslist-main {
		margin-left: 0;
	}

	.newslist-main li,
	.newslist-main li + li {
		margin-left: 0;
	}

	.newslist-main li:first-child .photo {
		float: none;
	}

	.newslist-main li:first-child .photo + .newstext {
		margin-top: 15px;
		margin-left: 0;
	}

	.blog_page .newslist-main li:first-child .photo + .newstext {
		margin-top: 15px;
		margin-left: 0;
	}

	.searchpage .search-result {
		margin-left: 0;
	}

	.text {
		margin-left: 0;
	}

	.text img[align="left"] {
		margin-left: 0;
	}

	.personsBlock .columns,
	.personsBlock .col {
		display: block;
	}

	.personsBlock .col {
		width: auto;
		padding-left: 0;
	}

	.lt-ie9 .personsBlock .col {
		border-left: none;
	}

	.personsBlock .col:first-child {
		border-right: none;
	}

	.videobl .author {
		float: none;
		margin: 0 0 15px 0;
		width: 90%;
	}

	.video {
		text-align: left;
	}

	.profile_page .button {
		margin-bottom: 0;
	}

	.profile_page .newslist-main li:first-child .photo {
		float: left;
	}

	.bloggers-hide {
		display: none;
	}

	.b-filterlist .searchli .search {
		margin-left: 0;
	}

	.profile .col {
		width: auto;
		display: block;
		float: none;
		padding: 10px 0;
	}

	.lt-ie9 .profile .col {
		border: none;
	}

	.profile .name a.profile_edite {
		display: block;
		float: none;
		left: 20px;
	}

	.profile_page .b-filter {
		width: 100%;
	}
}

@media screen and (min-width: 1200px) {
	.blogs-news ._optional {
		display: none !important;
	}

	.inner-main-events {
		margin-right: 240px;
	}

	.inner_sidebar {
		margin-top: -50px;
	}

	.content_sidebar .section-title a {
		-webkit-animation: blinkBlackGrey 1s .4s;
		animation: blinkBlackGrey 1s .4s;
	}

	.blogs-news .tab-links a,
	.blogs-news .tab-links a.active {
		display: inline-block;
		padding: 7px 0 0 0;
		border: none;
		color: black;
		cursor: default;
	}

	.blogs-news .tab-links {
		border: none;
	}

	.blogs-news .tab-box {
		margin-top: 0;
	}
}

