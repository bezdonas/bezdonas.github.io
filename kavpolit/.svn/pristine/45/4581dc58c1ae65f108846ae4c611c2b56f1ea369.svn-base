/* Animations */

@-webkit-keyframes sideVanish {
	0% {
		left: 20px;
	}

	99% {
		left: 70%;
	}

	100% {
		display: block;
	}
}

@keyframes sideVanish {
	0% {
		left: 0;
	}

	99% {
		left: 70%;
	}

	100% {
		display: block;
	}
}

@-webkit-keyframes blinkBlackGrey {
	0% {
		color: #868686;
	}

	25% {
		color: black;
	}

	50% {
		color: #868686;
	}

	75% {
		color: black;
	}

	100% {
		color: #868686;
	}
}

@keyframes blinkBlackGrey {
	0% {
		color: #868686;
	}

	25% {
		color: black;
	}

	50% {
		color: #868686;
	}

	75% {
		color: black;
	}

	100% {
		color: #868686;
	}
}

@-webkit-keyframes blinkGrayBlue {
	0% {
		color: #275e97;
	}

	25% {
		color: #c0c0c0;
	}

	50% {
		color: #275e97;
	}

	75% {
		color: #c0c0c0;
	}

	100% {
		color: #275e97;
	}
}

@keyframes blinkGrayBlue {
	0% {
		color: #275e97;
	}

	25% {
		color: #c0c0c0;
	}

	50% {
		color: #275e97;
	}

	75% {
		color: #c0c0c0;
	}

	100% {
		color: #275e97;
	}
}

/* /Animations */