/* Base
----------------------------------------------------------------------------------*/

/* Layout */
body {
	/*width: 320px;
	margin: 0 auto;*/
	font: 12px/1.3 sans-serif;
	color: #000;
	overflow-x: hidden;
}
/* /Layout */

/* Typo-tags */
h1 {
	font: 700 14px/1.5 'Open Sans', sans-serif;
	margin: 0 0 10px;
	text-transform: uppercase;
}
h2 {
	font: 600 11px/1.5 'Open Sans', sans-serif;
	margin: 0 0 10px;
	text-transform: uppercase;
}
p {
	margin-top: 0;
}
a {
	color: #3ba7ca;
}
a:active {
	color: #3bc5f1;
}

ol {
	padding-left: 15px;
	font-weight: 700;
}
ol li {
	margin-bottom: 5px;
	padding-left: 5px;
}
ol span {
	font-weight: 400;
}
/* /Typo-tags */

/* Font sizes */
.__font-s {
	font-size: 10px;
}
.__font-xs {
	font-size: 11px;
}
.__font-m {
	font-size: 12px;
}
.__font-xm {
	font-size: 13px;
}
.__font-x {
	font-size: 14px;
}
.__font-xl {
	font-size: 15px;
}
.__font-xxl {
	font-size: 16px;
}
/* /Font sizes */

/* Font boldness */
.__bolder, strong {
	font-weight: 700;
}
.__bold {
	font-weight: 800;
}
/* /Font boldness */

/* Colors */
.__light {
	color: #231f20;
}
/* /Colors */

/* Table-like block */
.columns {
	display: table;
	width: 100%;
}
.columns_col {
	display: table-cell;
}
/* /Table-like block */

/* Inline-dlist */
.dlist {
	margin: 0 0 20px;
}
.dlist_def {
	font-weight: bold;
	display: inline;
}
.dlist_def:before {
	content: '';
	display: block;
}
.dlist_dsc {
	display: inline;
	margin-left: 0;
}
/* /Inline-dlist */

/* Icons */
.ico {
	background: url('../i/icons.png') no-repeat 0 0;
	display: inline-block;
	margin-right: 5px;
}
	/* social */
	.ico.__social {
		width: 20px;
		height: 20px;
		margin: 0;
	}
	.ico.__twitter {
		background-position: 0 -20px;
	}
	.ico.__linkedin {
		background-position: -20px -20px;
	}
	.ico.__vk {
		background-position: -40px -20px;
	}
	.ico.__fb {
		background-position: -60px -20px;
	}
	/* map management */
	.ico.__button {
		width: 20px;
		height: 20px;
		position: relative;

		border: 1px solid #8ea1b7;
		border-radius: 5px;

		cursor: pointer;

		background: #ffffff; /* Old browsers */
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffffff), color-stop(100%,#e8ecf0)); /* Chrome,Safari4+ */
		background: -webkit-linear-gradient(top,  #ffffff 0%,#e8ecf0 100%); /* Chrome10+,Safari5.1+ */
		background: linear-gradient(to bottom,  #ffffff 0%,#e8ecf0 100%); /* W3C */
	}
	.ico.__button:active {
		background: #e8ecf0; /* Old browsers */
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#e8ecf0), color-stop(100%,#e8ecf0)); /* Chrome,Safari4+ */
		background: -webkit-linear-gradient(top,  #e8ecf0 0%,#e8ecf0 100%); /* Chrome10+,Safari5.1+ */
		background: linear-gradient(to bottom,  #e8ecf0 0%,#e8ecf0 100%); /* W3C */
	}
	.ico.__button:after {
		content: '';
		background: url('../i/icons.png') no-repeat 0 0;
		width: 20px;
		height: 20px;
		top: 0;
		left: 0;
		border-radius: 5px;
		position: absolute;
	}
	.ico.__minus:after {
		background-position: 0 -80px;
	}
	.ico.__plus:after {
		background-position: -20px -80px;
	}
	.ico.__left:after {
		background-position: -40px -80px;
	}
	.ico.__up:after {
		background-position: -60px -80px;
	}
	.ico.__down:after {
		background-position: -80px -80px;
	}
	.ico.__right:after {
		background-position: -100px -80px;
	}
	/* others */
	.ico.__r-arr {
		width: 7px;
		height: 10px;
		margin-top: 2px;
		background-position: 0 0;
	}
	.ico.__skype {
		width: 12px;
		height: 12px;
		margin-top: 3px;
		background-position: -20px 0;
	}
	.ico.__envelope {
		width: 13px;
		height: 10px;
		margin-top: 3px;
		background-position: -40px 0;
	}
/* /Icons */

/* Modal */
.modal {
	background: #f6f6f6;
	text-align: center;
	padding: 50px 20px;
}
/* /Modal */

/* Form elements */
.text-input {
	border: 1px solid #cdcfd0;
	width: 100%;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}

.button {
	font: 400 14px/1 'Open Sans', sans-serif;
	color: #fff;
	text-decoration: none;

	position: relative;
	display: inline-block;
	padding: 6px 10px;
	border-radius: 5px;
	border: 0;

	background: #58caef; /* Old browsers */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#58caef), color-stop(100%,#1991c9)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #58caef 0%,#1991c9 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #58caef 0%,#1991c9 100%); /* Opera 11.10+ */
	background: linear-gradient(to bottom,  #58caef 0%,#1991c9 100%); /* W3C */

	-webkit-box-shadow: 0 3px 3px 0 rgba(0,0,0,.3);
	box-shadow: 0 3px 3px 0 rgba(0,0,0,.3);

	-webkit-transition: ;

	text-shadow: 1px 1px rgba(0,0,0,.2);
}
.button:active {
	color: #fff;
	box-shadow: none;
	top: 3px;
	bottom: -3px;
}
/* /Form elements */

/* /Base 
----------------------------------------------------------------------------------*/


/* Project
----------------------------------------------------------------------------------*/

/* Header */
.header_wrp {
	padding: 20px 6.25% 17px;
}
.header_block {
	display: inline-block;
	vertical-align: top;
}
/* /Header */

/* Logo */
.logo {
	margin-top: -10px;
	width: 40%;
}
/* /Logo */

/* Contacts */
.contacts {
	text-align: right;
	width: 60%;
}
.contacts_phone {
	font: 800 16px/1 'Open Sans', sans-serif;
}
.contacts_schedule {
	font: 11px/1 sans-serif;
	color: #231f20;
	margin: 5px 0 0;
}
/* /Contacts */

/* Menu */
.menu {
	margin-bottom: 15px;
}
.menu_ul {
	padding: 0;
	margin: 0;
	margin-right: -2px;
}
.menu_li {
	width: 50%;
	display: inline-block;
	margin: 0;
}
.menu_lnk {
	font: 700 14px/1 'Open Sans', sans-serif;
	color: #fff;
	background: #37c2ef;
	text-decoration: none;
	text-align: center;
	display: block;
	margin-top: 2px;
	margin-right: 2px;
	padding: 8px 0;
}
.menu_lnk:active, .menu_lnk.__active {
	color: #fff;
	background: #2b6073;
}
.menu_lnk.__active {
	cursor: default;
	pointer-events: none;
}
	.menu.__main {}
	.menu.__main .menu_ul {}
	.menu.__main .menu_li {
		width: 100%;
	}
	.menu.__main .menu_lnk {
		text-transform: uppercase;
		display: block;
		text-align: left;
		padding: 8px 6.25%;
	}
	.menu.__main .menu_lnk:after {
		content: '';
		display: block;
		float: right;
		width: 7px;
		height: 10px;
		margin-top: 2px;
		background: url('../i/icons.png') no-repeat 0 0;
	}
/* /Menu */

/* Content */
.content {}
.content_wrp {
	padding: 0 6.25% 20px;
}
/* /Content */

/* Footer */
.footer_wrp {
	padding: 20px 6.25%;
	background: #f6f6f6;
}
.footer_block {
	display: inline-block;
	vertical-align: top;
}
/* /Footer */

/* Copyright */
.copyright {
	width: 65%;
}
.copyright_main {
	color: #6f7071;
	margin-bottom: 5px;
}
.copyright_sub {
	color: #939598;
	margin-bottom: 0;
}
/* /Copyright */

/* Links */
.links {
	width: 35%;
	text-align: right;
}
.links_full {}
.links_social {
	list-style-type: none;
	margin: 10px 0 0;
	padding: 0;
}
.links_li {
	display: inline-block;
}
.links_nota {
	display: inline-block;
	margin-top: 9px;
	width: 90px;
	height: 13px;
	background: url('../i/icons.png') no-repeat 0 -60px;
}
/* /Links */

/* CSS3 accordion */
.accordion {}
.accordion_checkbox {}
.accordion_button {
	display: block;
	position: relative;
	cursor: pointer;
	font: 800 12px/1 'Open Sans', sans-serif;
	text-transform: uppercase;
	background: #e6e5e5;
	padding: 9px 10px;
	margin-top: 1px;
}
.accordion_button:after {
	content: '';
	position: absolute;
	right: 9px;
	top: 5px;
	width: 20px;
	height: 20px;
	border-radius: 50%;
	background: #fff url('../i/icons.png') no-repeat 0 -40px;
	-webkit-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
	opacity: 0.9;
}
.accordion_article {
	box-shadow: inset 0px 3px 3px -1px rgba(0,0,0,.15);
	max-height: 0;
	overflow: hidden;
	-webkit-transition: max-height 0.3s ease-in-out;
	transition: max-height 0.3s ease-in-out;
	background: #f6f6f6;
}
.accordion_article-wrp {
	padding: 15px 10px;
}
.accordion_checkbox:checked ~ .accordion_article {
	display: block;
	max-height: 1000px;
	transition: max-height 1s ease-in-out;
}
.accordion_checkbox:checked ~ .accordion_button:after {
	-webkit-transform: rotate(180deg);
	transform: rotate(180deg);
	opacity: 1;
	-webkit-transition: all 0.3s ease-in-out;
	transition: all 0.4s ease-in-out;
}
	.accordion_button.__small {
		font: 800 10px/1 'Open Sans', sans-serif;
	}
/* /CSS3 accordion */

/* Order */
.order {
	background: #f6f6f6;
	padding: 11px;
}
.order_lbl,
.order_txt {
	display: block;
	color: #7b7b7c;
}
.order_lbl {
	margin-bottom: 2px;
}
.order_txt {
	margin-bottom: 5px;
}
.order_button {
	margin: 15px 0 5px;
}
/* /Order */

/* Tabs */
.tabs {
	display: inline-block;
	padding-left: 10px;
}
.tabs_i {
	display: inline-block;
	padding: 6px 8px;
	margin-right: 5px;
}
.tabs_i:active,
.tabs_i.__active {
	background: #e9e8e8;
}
.tabs_i:active .tabs_lnk {
	color: #231f20;
}
.tabs_i.__active .tabs_lnk {
	border: 0;
}
.tabs_lnk {
	text-decoration: none;
	color: #231f20;
	border-bottom: 1px dotted #231f20;
}
/* /Tabs */

/* Map */
.map {}
.map_management {
	position: relative;
	height: 30px;
}
.map_management .ico {
	margin: 0;
	position: absolute;
	top: 0;
}
.map_management .__minus {
	left: 0;
}
.map_management .__plus {
	left: 28px;
}
.map_management .__left {
	right: 126px;
}
.map_management .__up {
	right: 78px;
}
.map_management .__down {
	right: 52px;
}
.map_management .__right {
	right: 0;
}
.map_cont {
	height: 160px;
	margin-left: -10%;
	margin-right: -10%;
}
.map_cont .ymaps-glass-pane {
	box-shadow: inset 0 0 5px 0 rgba(0,0,0,.5) !important;
}
	
/* /Map */

/* /Project
----------------------------------------------------------------------------------*/


/* Cosmetic
----------------------------------------------------------------------------------*/

.__hidden {
	display: none !important;
}
.__nobm {
	margin-bottom: 0;
}
.__ib {
	display: inline-block;
}
.__disabled {
	opacity: 0.5;
	cursor: pointer;
	pointer-events: none;
}

/* /Cosmetic
