/* Base
----------------------------------------------------------------------------------*/

/* Layout */
body {
	/*width: 320px;
	margin: 0 auto;*/
	font: 12px/1.3 sans-serif;
	color: #000;
}
/* /Layout */

/* Typo-tags */
h1 {
	font: 700 14px/1.5 'Open Sans', sans-serif;
	margin: 0 0 10px;
	text-transform: uppercase;
}
p {
	margin-top: 0;
}
a {
	color: #3ba7ca;
}
a:active {
	color: #3bc5f1;
}
/* /Typo-tags */

/* Font sizes */
.__font-s {
	font-size: 10px;
}
.__font-sx {
	font-size: 11px;
}
.__font-m {
	font-size: 12px;
}
/* /Font sizes */

/* Colors */
.__light {
	color: #231f20;
}
/* /Colors */

/* Table-like block */
.columns {
	display: table;
	width: 100%;
}
.columns_col {
	display: table-cell;
}
/* /Table-like block */

/* Inline-dlist */
.dlist {
	margin: 0 0 20px;
}
.dlist_def {
	font-weight: bold;
	display: inline;
}
.dlist_def:before {
	content: '';
	display: block;
}
.dlist_dsc {
	display: inline;
	margin-left: 0;
}
/* /Inline-dlist */

/* Icons */
.ico {
	background: url('../i/icons.png') no-repeat 0 0;
	display: inline-block;
	margin-right: 5px;
}
	.ico.__social {
		width: 20px;
		height: 20px;
		margin: 0;
	}
	.ico.__twitter {
		background-position: 0 -20px;
	}
	.ico.__linkedin {
		background-position: -20px -20px;
	}
	.ico.__vk {
		background-position: -40px -20px;
	}
	.ico.__fb {
		background-position: -60px -20px;
	}
	.ico.__r-arr {
		width: 7px;
		height: 10px;
		margin-top: 2px;
		background-position: 0 0;
	}
	.ico.__skype {
		width: 12px;
		height: 12px;
		margin-top: 3px;
		background-position: -20px 0;
	}
	.ico.__envelope {
		width: 13px;
		height: 10px;
		margin-top: 3px;
		background-position: -40px 0;
	}
/* /Icons */

/* /Base 
----------------------------------------------------------------------------------*/


/* Project
----------------------------------------------------------------------------------*/

/* Header */
.header_wrp {
	padding: 20px 6.25% 17px;
}
.header_block {
	display: inline-block;
	vertical-align: top;
}
/* /Header */

/* Logo */
.logo {
	margin-top: -10px;
	width: 40%;
}
/* /Logo */

/* Contacts */
.contacts {
	text-align: right;
	width: 60%;
}
.contacts_phone {
	font: 800 16px/1 'Open Sans', sans-serif;
}
.contacts_schedule {
	font: 11px/1 sans-serif;
	color: #231f20;
	margin: 5px 0 0;
}
/* /Contacts */

/* Menu */
.menu {
	margin-bottom: 15px;
}
.menu_ul {
	padding: 0;
	margin: 0;
}
.menu_li {
	background: #37c2ef;
}
.menu_lnk {
	font: 700 14px/1 'Open Sans', sans-serif;
	color: #fff;
	background: #37c2ef;
	text-decoration: none;
	text-decoration: center;
}
.menu_lnk:active {
	color: #fff;
	background: #2b6073;
}
	.menu.__main {}
	.menu.__main .menu_ul {}
	.menu.__main .menu_li {
		margin-bottom: 2px;
	}
	.menu.__main .menu_lnk {
		text-transform: uppercase;
		display: block;
		padding: 8px 6.25%;
	}
	.menu.__main .menu_lnk:after {
		content: '';
		display: block;
		float: right;
		width: 7px;
		height: 10px;
		margin-top: 2px;
		background: url('../i/icons.png') no-repeat 0 0;
	}
/* /Menu */

/* Content */
.content {}
.content_wrp {
	padding: 0 6.25% 20px;
}
/* /Content */

/* Footer */
.footer_wrp {
	padding: 20px 6.25%;
	background: #f6f6f6;
}
.footer_block {
	display: inline-block;
	vertical-align: top;
}
/* /Footer */

/* Copyright */
.copyright {
	width: 65%;
}
.copyright_main {
	color: #6f7071;
	margin-bottom: 5px;
}
.copyright_sub {
	color: #939598;
	margin-bottom: 0;
}
/* /Copyright */

/* Links */
.links {
	width: 35%;
	text-align: right;
}
.links_full {}
.links_social {
	list-style-type: none;
	margin: 7px 0 0;
	padding: 0;
}
.links_li {
	display: inline-block;
	/*margin-right: 2px;*/
}
/* /Links */

/* /Project
----------------------------------------------------------------------------------*/


