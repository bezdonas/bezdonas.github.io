.main {
	position: relative; height: 100%; padding: 0 0 0 0;
	-moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box;
}
.content {
}
section.screen {
	width: 100%; height: 100%; position: relative; overflow: hidden; z-index: 1;
	-moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box;
}
.screen.current { z-index:2; }
.manyscreen section.screen { position: absolute; }


.manyscreen .main { overflow: hidden; }


.mainpage { background: #fff; }

.screenmain .mainpage { left: 0; top: 0; }
.manyscreen .mainpage { left: 0; top: 0; margin: 0; }

.logo_giant { 
	width: 61.25em; height: 5.1875em;
	position: absolute; left: 50%; bottom: 50%; margin: 0 0 1.35em -30.625em; z-index: 5;
}

.logo_giant object, .logo_giant embed, .logo_giant img, .logo_giant svg { width: 100%; height: 100%; }
.no-svg .logo_giant object, .no-svg .logo_giant embed { display: none; }


.b { position: absolute; width: 100%; left: 0; bottom: 50%; margin: 0 0 11.6em 0; z-index: 5; }
.b div:before, .b div:after { content: ""; display: table; }
.b div:after { clear: both; }
.b div { width: 61.25em; margin: 0 auto; *zoom: 1; }
.b a { float: right; outline: 0 none; }
.b span { display: inline-block; vertical-align: top; width: 12.5em; margin: 25px 11px .4em 0; }
.b:before { content: ' '; position: absolute; width: 3.75em; height: 0.1875em; background: #000; bottom: 100%; left: 50%; margin: 0 0 -35px -13em; }
.b strong { font: .937em/1.3em 'ProximaNova-Bold', Arial, sans-serif; text-transform: uppercase; display: block; margin: 0 0 6px 0; letter-spacing: .025em; }
            /* 11px/18px from 16px */
.b em { font: .687em/1.637em Tahoma, Arial, sans-serif; display: block; }

.slogan {
	font: 1.408em/1.243em 'ProximaNova-Bold', Arial, sans-serif; text-transform: uppercase; width: 13em;
	position: absolute; left: 50%; bottom: 50%; margin: 0 0 6.5em -18.861em; letter-spacing: .05em; z-index: 5;
}
.introducing {
	font: 0.875em/1.714em Tahoma, Arial, sans-serif; position: absolute; left: 50%; top: 50%; margin: 2em 0 0 -14.9em; width: 40.5em; z-index: 0;
}
.mainlocalnav {
	position: absolute; left: 50%; top: 50%; margin: 17.88em 0 0 -30em; width: 60em; text-align: center; z-index: 5;
}
.mainlocalnav a, .mainlocalnav a:link, .mainlocalnav a:visited {
	position: absolute; bottom: 0;  outline: 0;
	-moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box;
}
.mainlocalnav a.toleft { left: 0; text-align: left; width: 9em; }
.mainlocalnav a.toleft:hover, .mainlocalnav a.toleft:active { }
.mainlocalnav a.tobottom { left: 50%; margin-left: -5.75em; }
.mainlocalnav a.toright { right: 0; text-align: right; width: 9em; }
.mainlocalnav a.toright:hover, .mainlocalnav a.toright:active { }
.mainlocalnav a strong { text-transform: uppercase; display: block; font: .937em/1.3em 'ProximaNova-Bold', Arial, sans-serif; letter-spacing: .2em; margin-bottom: .3em; position: relative; }
.mainlocalnav a strong:before { content:' '; position: absolute; bottom: 100%; background-image: url('../i/icons.png'); background-repeat: no-repeat; }

.mainlocalnav a.toleft strong:before { background-position: -40px 0; width: 10px; height: 13px; left: 0; margin: 0 0 16px 9px; }

.mainlocalnav a.tobottom strong:before { background-position: -60px 0; width: 13px; height: 10px; left: 50%; margin: 0 0 17px -8px; }

.mainlocalnav a.toright strong:before { background-position: -80px 0; width: 10px; height: 13px; right: 0; margin: 0 9px 16px 0; }

.mainlocalnav a em { display: block; color: #9da2a8; font: italic .937em/1.3em 'Times New Roman', serif; }

.mainlocalnav .descr { display: none; font: 0.812em/1.385em Tahoma, Arial, sans-serif; position: absolute; bottom: 0; }
.mainlocalnav .descr span { bottom: 0; }
.mainlocalnav .toleft .descr span { display: block; width: 9.75em; padding: 1em 1.2em 7.75em 1.45em; left: 0; }
.mainlocalnav .toleft.hover .descr { 
	display: block; margin: 0 0 -0.3em -1.65em; border: solid 3px #0f63a1; left: 0; }
.mainlocalnav .tobottom .descr span { display: block; width: 15.5em; padding: 1em 2.25em 1.85em 13.5em; left: 0; }
.mainlocalnav .tobottom.hover .descr { 
	display: block; text-align: left; margin: 0 0 -.3em -13em; border: solid 3px #199a78; left:100%; 
}
.mainlocalnav .toright .descr span { display: block; width: 9.5em; padding: 1em 1.2em 6.45em 1.65em; right: 0; }
.mainlocalnav .toright.hover .descr { 
	text-align: left; display: block; border: solid 3px #89af40; margin: 0 -1.65em -.3em 0; right: 0; }

.hoverstyle {
	position: absolute; width: 100%; height: 100%; left: 0; top: 0; z-index: 3; overflow: hidden;
}
.csstransitions .hoverstyle {
	-webkit-transition: all .35s;
	-moz-transition: all .35s;
	-ms-transition: all .35s;
	-o-transition: all .35s;
	transition: all .35s;
}
.hoverstyle.toleft {
	width: 0; opacity: 0; filter: alpha(opacity=0);
}
.hoverstyle.toleft.hover { width: 100%; opacity: 1; filter: alpha(opacity=100); }
.hoverstyle.tobottom { height: 0; bottom:0; top:auto; opacity: 0; filter: alpha(opacity=0); 
}
.hoverstyle.tobottom.hover { height: 100%; width:100%; opacity: 1; filter: alpha(opacity=100); }
.hoverstyle.toright { width: 0; left:auto; right:0; opacity: 0; filter: alpha(opacity=0); 
}
.hoverstyle.toright.hover { width: 100%; opacity: 1; filter: alpha(opacity=100); }
.hoverstyle > div {
	width: 100%; height: 100%; overflow: hidden; position: relative; 
}
.hoverstyle.toleft .bg {
	width: 100%; height: 100%; margin: 0 -50% 0 0; min-width: 1240px; min-height: 834px; position: absolute;
	background: url('../i/main_hover_1.png') no-repeat 50% 50%; right: 50%; top: 0; 
}
.hoverstyle.toleft .aside {
	width: 100%; height: 100%; position: absolute; top: 0; right: 100%; margin: 0 -3.75em 0 0; background: #000; z-index: 1;
}
.hoverstyle.toleft .aside img { height: 100%; position: absolute; right: 0; top: 0; }
.hoverstyle.tobottom .bg {
	width: 100%; height: 0; margin: 0 0 0 0; min-width: 980px; min-height: 834px; position: absolute;
	background: url('../i/main_hover_2.png') no-repeat 50% 50%; left: 0; bottom: 0;
}
.hoverstyle.toright .bg {
	width: 100%; height: 100%; margin: 0 0 0 -50%; min-width: 1240px; min-height: 834px; position: relative;
	background: url('../i/main_hover_3.png') no-repeat 50% 50%; left: 50%; top: 0;
}
.hoverstyle.toright .aside {
	width: 100%; height: 100%; position: absolute; top: 0; left: 100%; margin: 0 0 0 -3.75em; background: #60a64a; z-index: 1;
}

/*  */

.mainspecial { background: #65ae4b url('../i/main_specialization_bg_1500.png') no-repeat 50% 50%; }
@media only screen and (min-width: 1500px) {
	.mainspecial { background-image: url('../i/main_specialization_bg_2200.png'); }
}
@media only screen and (min-width: 1900px) {
	.mainspecial { background-image: url('../i/main_specialization_bg_3000.png'); }
}


.manyscreen .mainspecial { left: 0; top: 0; margin: 0 0 0 100%; }

.mainspecial a, .mainspecial a:link, .mainspecial a:visited { color: #fff; }

.mainspecial .rowtop { width: 60em; position: absolute; left: 50%; bottom: 50%; margin: 0 0 1em -30em; height: 36%; }

.mainspecial .rowbottom { width: 100%; position: absolute; left: 0; top: 50%; margin: .8em 0 0 0; height: 38%; }

.mainspecial h1, .mainspecial h2 { font: 1.6875em/1.2em 'ProximaNova-Bold', Arial, sans-serif; text-transform: uppercase; letter-spacing: .05em; }
.mainspecial h1 { width: 17em; text-align: center; margin: 0 auto; height: 3.95em; }

.mainspecial .aboutspec > ul { list-style: none; margin: 0 0 0 -2.85em; padding: 0; }
.mainspecial .aboutspec > ul > li { margin: 0 0 0 2.85em; padding: 0; font: 1.0625em/1.3em 'ProximaNova-Bold', Arial, sans-serif; text-transform: uppercase; display: inline-block; letter-spacing: .05em; }
.mainspecial .aboutspec > ul > li .hover { display: none; }

.mainspecial .descr { font: .937em/1.4em Tahoma, Arial, sans-serif; width: 36em; margin: 0 auto; text-align: center; position: relative; }
.mainspecial .descr .valignw { height: 9em; }

.mainspecial .descr:after { content: ' '; position: absolute; width: 3.9em; height: 0.1875em; background: #000; top: 100%; left: 50%; margin: .65em 0 0 -1.95em; }

.mainspecial h2 { margin: 0 0 0; }

.mainspecial .serv { width: 30em; position: relative; left: 50%; top: 0; margin-left: -29.375em; }
.mainspecial .serv h2 { margin-bottom: 1.1em; }
.mainspecial .serv p { font: .75em/1.5em Tahoma, Arial, sans-serif; width: 13em; display: inline-block; vertical-align: top; margin: 0 0 1.75em 0; }
.mainspecial .serv p.more { width: 21em; margin-left: 13.3em; }
.mainspecial .serv p a { display: block; font-size: 1.25em; margin: 0 0 .35em; }


.mainspecial .case { position: absolute; width: 60%; left: 50%; top: 0; margin: 0 0 0 -.625em; }
.mainspecial .case h2 { margin-left: 10.35em; }
.mainspecial .casecarousel { list-style: none; margin: -1.55em 0 0 0; padding: 2.85em 0 0 0; height: 12.5em; overflow: hidden; position: relative; }
.mainspecial .ccwrapper { margin: -1.55em 0 0 0; padding: 2.85em 0 0 0; height: 12.5em; position: relative; }
.mainspecial .ccwrapper .casecarousel { margin: 0; padding: 0 0 0 0; height: auto; overflow: visible; position: static; }
.mainspecial .ccframe { overflow: hidden; position: relative; width: 100%; height: 12em; }
.mainspecial .casecarousel li { margin: 0 0 0 0; padding: .5em 0 .5em 18.65em; font: .937em/1.4em Tahoma, Arial, sans-serif; }
.mainspecial .casecarousel li.current { background: #2ea068; }
.mainspecial .casecarousel li span { display: block; width: 14.5em; }
.mainspecial .casecarousel li img { display: none; width: 16em; }
.mainspecial .casecarousel li.current img { display: block; position: absolute; top: 0; left: 1.35em; }
.mainspecial .ccwrapper .casecarousel li.current img { display: none; }
.mainspecial .ccimgcont { position: absolute; top: 0; left: 1.35em; width: 15em; height: 14.3125em; overflow: hidden; }
.mainspecial .ccimgcont img { width: 15em;  height: 14.3125em; }
.mainspecial .ccnav { position: absolute; top: 100%; left: 20em; }
.mainspecial .ccnext, .mainspecial .ccprev { display: inline-block; width: 20px; height: 14px; background: url('../i/icons.png') no-repeat -97px 0; cursor: pointer; margin: 0 .5em 0 0; }
.mainspecial .ccprev { background-position: -117px 0; }

.hoverlayer { position: absolute; top: 0; left: 0; height: 100%; z-index: 15; }
.hoverlayer.haboutspec { background: #fff; width: 14em; left: -1000em; height: 0; overflow: hidden; margin: 0 0 0 -2.55em; }
.haboutspec.hover { height: 100%; }
.haboutspec > img { width: 100%; opacity: 1; filter: alpha(opacity=100); }
.haboutspec.hover > img { width: 100%; opacity: 1; filter: alpha(opacity=100); }
.haboutspec .txt { padding: 0 2.55em; position: absolute; left: 0; bottom: 50%; margin: 0 0 1em 0; height: 36%; width: 100%;
	-moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box;
}
.haboutspec h3 { font: 1.0625em/1.3em 'ProximaNova-Bold',Arial,sans-serif; text-transform: uppercase; letter-spacing: 0.05em; position: relative; margin: 6.3em 0 .6em 0; }
.haboutspec h3 span { font-family: 'ProximaNova-Regular',Arial,sans-serif; display: block; }
.haboutspec h3 img { position: absolute; bottom: 100%; left: 0; margin: 0 0 .75em 0; }
.haboutspec .descr, .haboutspec .txt ul, .haboutspec .more { font: .75em/1.5em Tahoma, Arial, sans-serif; }
.haboutspec .descr { min-height: 6em; margin: 0 0 1em; }
.haboutspec .txt ul { list-style: none; margin: 1.5em 0 2.5em 0; padding: 1.55em 0 .1em 0; border-top: solid 1px #a8a8a8; border-bottom: solid 1px #a8a8a8; }
.haboutspec .txt li { margin: 1.325em 0; padding: 0; }
.haboutspec .txt li strong { display: block; }
.haboutspec .more { margin: 2em 0 1em 0; }
.haboutspec .more a, .haboutspec .more a:link, .haboutspec .more a:visited {
	color: #0b65a4;
}


/* */
.mainportfolio {}
.manyscreen .mainportfolio { left: 0; top: 0; margin: 0 0 0 -100%; }

.imgset, .imgset .itemshadow { width: 100%; height: 100%; position: absolute; left: 0; top: 0; }
.imgset { z-index: 0; }
.lt-ie9 .imgset { background: #333; opacity: 0; filter: alpha(opacity=0); }
.imgset.current, .lt-ie9 .imgset.current { z-index: 1; display: block; opacity: 1; filter: alpha(opacity=100); }
.imgset .item { width: 100%; height: 100%; overflow: hidden; min-width: 980px; min-height: 834px; }
.flexbg { position: absolute; top: 50%; left: 50%; margin: -50% 0 0 -50%; width: 100%; z-index: 0; }
.imgset .itemshadow { background: url('../i/gradient_vert_shadow.png') repeat-x 0 100%; }

.aboutitem { width: 50em; position: absolute; left: 50%; bottom: 0; margin: 0 0 11em -25em; z-index: 1; text-align: center; }
.csstransitions .imgset.inaction, .csstransitions .imgset.inaction .flexbg, .csstransitions .imgset.inaction .aboutitem {
	-webkit-transition: all .75s;
	-moz-transition: all .75s;
	-ms-transition: all .75s;
	-o-transition: all .75s;
	transition: all .75s;
}
.aboutitem .type { font: 1.0625em/1.3em 'ProximaNova-Bold',Arial,sans-serif; text-transform: uppercase; letter-spacing: 0.05em; color: #fff; position: relative; }
.aboutitem .type:before, .aboutitem .type:after { content: ' '; width: 4.59em; height: 0.1em; background: #fff; display: inline-block; margin: 0 1.2em; vertical-align: middle; }
.aboutitem .type:before { }
.aboutitem .title { font: 4.210625em/0.963em 'ProximaNova-Extrabld',Arial,sans-serif; text-transform: uppercase; color: #fff; margin: .4em 0 .725em }
.aboutitem .more { }
.aboutitem .more a { display: block; margin: 0 auto; width: 220px; height: 40px; background: url('../i/icons.png') no-repeat -40px -20px; text-indent: -9999em; }

.portfoliomenu { position: absolute; z-index: 5; top: 5.75em; left: 2.95em; }
.portfoliomenu a, .portfoliomenu a:link, .portfoliomenu a:visited { color: #fff; font: .937em/1.3em 'ProximaNova-Regular', Arial, sans-serif; letter-spacing: .025em; display: inline-block; }

.portfoliomenu .carnav { margin: 4em 0 0 -8px; }
.portfoliomenu .carnav a, .portfoliomenu .carnav a:link, .portfoliomenu .carnav a:visited {
	width: 20px; height: 20px; display: block; background: url('../i/icons.png') no-repeat -140px 0; margin: 0 0 20px; outline: 0;
}
.portfoliomenu .carnav a.current, .portfoliomenu .carnav a.current:link, .portfoliomenu .carnav a.current:visited {
	background-position: -160px 0;
}

