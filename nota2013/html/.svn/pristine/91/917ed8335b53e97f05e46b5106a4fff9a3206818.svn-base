/* ===== Primary Styles ========================================================
   Author:
   ========================================================================== */

/* +++ NAVIGATION +++ */

.mainmenu { margin: 0 0 0 -14.985em; position: absolute; left: 50%; top: 0; height: 100%; font: .937em/1.3em 'ProximaNova-Regular', Arial, sans-serif; letter-spacing: .025em; }
.mainmenu ul { padding: 0; margin: 0; height: 100%; }
.mainmenu li { display: inline-block; padding: 0; vertical-align: top; margin: 0; padding: 1em .985em 0 .985em; height: 100%; -moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box; }
.mainmenu li:first-child { }
.mainmenu a, .mainmenu a:link, .mainmenu a:visited { text-decoration: none; }

.mainmenu .current { border-bottom: solid 1px #000; }
.mainmenu .current.item { background: #000; }
.mainmenu .current a, .mainmenu .current a:link, .mainmenu .current a:visited { }
.mainmenu .current.item a, .mainmenu .current.item a:link, .mainmenu .current.item a:visited { color: #fff; }

.mainmenu a:hover, .mainmenu a:active { }

.manyscreen .mainmenu a, .manyscreen .mainmenu a:link, .manyscreen .mainmenu a:visited { color: rgb(255,255,255); }
.mainmenu a, .mainmenu a:link, .mainmenu a:visited,
.screenmain .mainmenu a, .screenmain .mainmenu a:link, .screenmain .mainmenu a:visited { color: rgb(0,0,0); }
.csstransitions .mainmenu a, .csstransitions .mainmenu a:link, .csstransitions .mainmenu a:visited {
  -webkit-transition: color .35s;
  -moz-transition: color .35s;
  -o-transition: color .35s;
  -ms-transition: color .35s;
  transition: color .35s;
}


/* Inner menu */

.innermenu {
	width: 100%;
	background: black;
	font: .937em/1.3em 'ProximaNova-Regular', Arial, sans-serif;
	color: white;
}
.innermenu,
.innermenu ul,
.innermenu li,
.innermenu a {
	height: 3.125em;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}
.innermenu li,
.innermenu a,
.innermenu .second-level {
	display: inline-block;
	vertical-align: top;
}
.innermenu li.float {
	float: right;
	margin-top: -3.125em;
}
.innermenu .first-level {
	letter-spacing: .025em;
	margin: 0 1.015em 0 2.05em;
}
.innermenu .first-level > li {
	display: table-cell;
}
.innermenu li {
	height: 100%;
	position: relative;
}
.innermenu a {
	margin: 0;
	padding: 0 .985em;
	line-height: 3.4em;
}

.innermenu .ico-star {
	display: inline-block;
	vertical-align: middle;
	width: 16px;
	height: 14px;
	/*'../i/star.png'*/
	background: url('data:image/png;base64,')
	0 0 no-repeat;
}

/* hover, active states */
.innermenu .second-level {
	width: 0;
	overflow: hidden;
	height: 3.25em;
}
.csstransitions .innermenu .second-level {
	-webkit-transition: width .25s;
	transition: width .25s;
}
.innermenu li.active > a {
	font-family: 'ProximaNova-Bold', Arial, sans-serif;
}
.csstransitions .innermenu li.section {
	-webkit-transition: color .25s;
	transition: color .25s;
}
.innermenu li:hover.section > a,
.innermenu li.section.active > a {
	font-family: 'ProximaNova-Bold', Arial, sans-serif;
	color: #989898;
}
.innermenu .section:hover > a:after,
.innermenu .section.active > a:after {
	content: ':';
}

.innermenu .second-level li:before,
.innermenu li:not(.section):before {
	content: '';
	position: absolute;
	left: .985em;
	right: 100%;
	bottom: 0;
	height: .125em;
	background: black;
}
.csstransitions .innermenu .second-level li:before,
.csstransitions .innermenu li:not(.section):before {
	-webkit-transition: right .2s;
	transition: right .2s;
}
.innermenu .second-level .active:before,
.innermenu .second-level li:hover:before,
.innermenu li:not(.section).active:before,
.innermenu li:not(.section):hover:before {
	right: .985em;
}

/* Modification for darkheader */
.header.darkheader .innermenu {
	background: #f5f5f5;
}
.header.darkheader .innermenu {
	color: black;
}

/* /Inner menu */

.soc { font: .875em/1.4em 'WebSymbolsLigaRegular'; position: absolute; top: 1.05em; right: 2em; }
.soc a, .soc a:link, .soc a:visited { margin: 0 .494em; text-decoration: none; }
.soc a:hover, .soc a:active { }
	
.manyscreen .soc a, .manyscreen .soc a:link, .manyscreen .soc a:visited { color: #fff; }
.soc a, .soc a:link, .soc a:visited, 
.screenmain .soc a, .screenmain .soc a:link, .screenmain .soc a:visited { color: #000; }

.screennav { width: 11.875em; min-height: 8.5em; position: absolute; right: 0; top: 3.125em; line-height: 1; background: rgba(0,0,0,0); z-index: 10; text-align: center;
}
.csstransitions .screennav {
  -webkit-transition: background .25s;
  -moz-transition: background .25s;
  -o-transition: background .25s;
  -ms-transition: background .25s;
  transition: background .25s;
}
.screenmain .screennav { display: none; }
.nextmain .screennav, .prevmain .screennav { opacity: 0; filter: alpha(opacity=0); 
}
.csstransitions .nextmain .screennav, .csstransitions .prevmain .screennav {
  -webkit-transition: opacity .5s;
  -moz-transition: opacity .5s;
  -o-transition: opacity .5s;
  -ms-transition: opacity .5s;
  transition: opacity .5s;
}
.no-touch .screennav.hover { padding: 0 0 4em; background: rgba(0,0,0,.45); }
.no-rgba .screennav.hover { background: url('../i/screennavbg.png') repeat 0 0; }
.screennav > div { padding: 0 2em 0 2.625em; }
.screennav div { display: inline-block; vertical-align: top; }
.screennav .sleft { margin: 3.125em .625em .625em 0; }
.screennav .smain { margin: 3.125em .625em .625em 0; }
.screennav .sright { margin: 3.125em .625em .625em 0; }
.screennav .sbottom { margin: 0 .625em .625em 0; }
.screennav a { outline: 0; }
.screennav .square { display: block; border: solid 2px #fff; padding: 2px; width: 1em; height: 1em; }
.screennav .square i { 
	display: block; width: 100%; height: 100%; background: #fff; opacity: 0; filter: alpha(opacity=0);
}
.csstransitions .screennav .square i { 
  -webkit-transition: opacity .25s;
  -moz-transition: opacity .25s;
  -o-transition: opacity .25s;
  -ms-transition: opacity .25s;
  transition: opacity .25s;
}
.screennav .sright i, .no-touch .screennav > div > .sright:hover i { background: #9fc14b; }
.screennav .sbottom i, .no-touch .screennav > div > .sbottom:hover i { background: #17987d;}
.screennav .sleft i, .no-touch .screennav > div > .sleft:hover i { background: #0d6aa8; }
.screennav .current i { background: #fff; opacity: 1; filter: alpha(opacity=100); }

.screennav .title { width: 0; overflow: hidden; position: absolute; bottom: 0; left: 0; padding: 0 0 1.75em 0; opacity: 0; filter: alpha(opacity=0);
}
.csstransitions .screennav .title { 
  -webkit-transition: width .5s;
  -moz-transition: width .5s;
  -o-transition: width .5s;
  -ms-transition: width .5s;
  transition: width .5s;
}
.screennav .title strong { width: 12.5em; display: block; text-align: center; font: .937em/1.3em 'ProximaNova-Bold', Arial, sans-serif; text-transform: uppercase; color: #fff;	 }
.screennav .sleft .title strong { color:#0d6aa8; }
.screennav .smain .title strong { }
.screennav .sright .title strong { color:#9fc14b; }
.screennav .sbottom .title strong { color:#17987d; }
.screennav .title em { width: 12.5em; display: block; text-align: center; font: italic .937em/1.3em 'Times New Roman', serif; color: #fff; }
.no-touch .screennav > div > div:hover .title { width: 100%; opacity: 1; filter: alpha(opacity=100); }
.no-touch .screennav > div > div:hover i { opacity: 1; filter: alpha(opacity=100); }

/* --- NAVIGATION --- */


/* +++ HEADER +++ */
.header {
	position: absolute; height: 3.125em; width: 100%; left: 0; top: 0; z-index: 10; 
	border-bottom: solid 1px #e8e8e8;
	-webkit-box-shadow:  0px 1px 2px 0px #eeeeee; box-shadow:  0px 1px 2px 0px #eeeeee;
}
header { position: relative; z-index: 1; width: 100%; height: 100%; }
.header_bg {
	position: absolute; top: 0; left:0; width: 100%; height: 100%; z-index:0;
	background: #0c273e;
}
.screenmain .header, .manyscreen .header {
	border-bottom: 0;
	-webkit-box-shadow:  none; box-shadow:  none;
}
.screenmain .header_bg { width: 0; }
.screenmain .logo { opacity: 0; }
.lt-ie9 .screenmain .logo { filter: alpha(opacity=0); }

.logo { position: absolute; top: .937em; left: 2.95em; width: 13.5em; height: 1.1875em; margin: 0; opacity: 1; }
.lt-ie9 .logo { filter: alpha(opacity=100);}
.logo object, .logo embed, .logo img { width: 100%; height: 100%; }
.no-svg .logo object, .no-svg .logo embed { display: none; }

/* Dark header modification */
.header.darkheader {
	background: #0c273e;
}
.header.darkheader .soc a,
.header.darkheader .mainmenu a,
.header.darkheader .mainmenu a:link,
.header.darkheader .mainmenu a:visited {
	color: white;
}
.header.darkheader .mainmenu .current.item {
	background: #f5f5f5;
	border: none;
}
.header.darkheader .mainmenu .current.item a {
	color: black;
	font-family: 'ProximaNova-Bold', Arial, sans-serif;
}

/* Opaque modification. Works only for box-shadow support */
.header.opaque {
	box-shadow: none;
	border: none;
}
.boxshadow .header.opaque .mainmenu li {
	background: #0c273e;
}
.boxshadow .header.opaque {
	background-color: transparent;
}
.boxshadow .header.opaque .mainmenu {
	box-shadow: -9000px -9000px 0 9000px #0c273e, 9000px -9000px 0 9000px #0c273e;
	z-index: -1;
}
.boxshadow .header.opaque .mainmenu .current.item {
	background: transparent;
}
