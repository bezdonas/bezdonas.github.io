/* +++ FOOTER +++ */
.footer {
	position: relative; height: 4.375em; width: 100%;
}
footer { position: absolute; width: 30.125em; left: 50%; bottom: 1.9em; margin: 0 0 0 -13em; }
footer div { float: left; margin: 0 47px 0 0; font: .687em/1.185em Tahoma, Arial, sans-serif; }

.present { position: absolute; left: 50%;  bottom: 0; width: 61.25em; margin: 0 0 0 -30.625em; }
.present div { position: absolute; right: 0;  bottom: 1.9em; }
.present a { font: .687em/1.185em Tahoma, Arial, sans-serif; display: block; width: 11.55em; position: relative; }
.present a span { display: block; position: absolute; top: 100%; left: 0; color: #fff; margin: .4em 0 0 0; }
.present a:before { content: ' '; width: 29px; height: 37px; background: url('../i/icons.png') no-repeat 0 0; position: absolute; right: 100%; bottom: 0; margin: 0 10px 3px 0; }
.screenright .present a:before, .screenbottom .present a:before { background-position: 0 -40px; }

.manyscreen .footer, .manyscreen .footer a, .manyscreen .footer a:link, .manyscreen .footer a:visited { color: rgb(255,255,255); }
.csstransitions .footer, .csstransitions .footer a, .csstransitions .footer a:link, .csstransitions .footer a:visited {
  -webkit-transition: color .35s;
  -moz-transition: color .35s;
  -o-transition: color .35s;
  -ms-transition: color .35s;
  transition: color .35s;
}
.footer, .footer a, .footer a:link, .footer a:visited,
.screenmain .footer, .screenmain .footer a, .screenmain .footer a:link, .screenmain .footer a:visited,
.screenbottom .footer, .screenbottom .footer a, .screenbottom .footer a:link, .screenbottom .footer a:visited { color: rgb(0,0,0); }
.screenmain .present a span { color: #9da2a8; }


