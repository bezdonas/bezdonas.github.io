	}
.filtrMap a:first-child {border-right:1px solid	#e3e3e3;}
.filtrMap a:hover, .filtrMap a.act {color:#ffffff; background:#1966a5;}

.filtrOffice .filtrBlock dl {background:#ffffff;}
.filtrOffice dl {
    margin-bottom: 1.875em;
	padding: 1.57em 1.875em 1.125em 1.875em;
}
.filtrOffice dl dt {
    margin-bottom: 0.1875em;
	color:#acacac;
	font: 0.75em/1.875em Tahoma, Arial, sans-serif; 
}

.filtrOffice dl dd {
    color: #000000;
    font: 1em/1.688em Tahoma, Arial, sans-serif;
	margin:0 0 0.625em 0;
}
.filtrOffice dl dd a {
	color:#1966a5;
}

.ui-tabs-nav {
	margin:0;
	padding:0;
	list-style-type:none;
	}
.ui-tabs-nav li {
	display:inline-block;
	vertical-align:top;
	width:8em;
	text-align:center;
	background: rgba(255,255,255,.5);
	}
.ui-tabs-nav li a {
	display:inline-block;
	vertical-align:top;
	text-align:center;
	font: 0.75em/1.875em Tahoma, Arial, sans-serif; 
	padding:0.685em 0;
	color:#1966a5;
