.work_team dd {
	margin: 0;
	color: #acacac;
}
.work_team ul {
	list-style-type: none;
	margin: 5px 0 0 0;
	padding-left: 0;
}
.work_team ul li {
	font-size: 1em;
}
.work_side {
	display: none;
	position: absolute;
	top: 113px;
	right: 50px;
	z-index: 100;
}

/* Cascade mods */
.work .pprev,
.work .pnext {
	top: 10em;
	width: 52px;
}
.col .work_comment {
	padding-top: 65px;
}

@media only screen and (min-width: 1600px) {
	.work_side,
	.work_side .work_reward,
	.work_side .work_details {
		display: block;
	}
	.work_reward,
	.work_details {
		display: none;
	}

	.work_reward {
		float: left;
	}
	.work_reward img {
		display: block;
		margin-bottom: 10px;
	}
	.work_reward-label {
		vertical-align: top;
		margin-top: 10px;
	}
	.work_details {
		clear: both;
		width: auto;
	}
	.work_reward + .work_details {
		border-top: 2px solid black;
		padding-top: 20px;
	}
	.work_tags {
		float: left;
	}
	.work_client-works {
		float: none;
		clear: left;
		padding-top: 20px;
	}
}

