	}
.ui-tabs-nav li.ui-tabs-active {background:#ffffff;}
.ui-tabs-nav li.ui-tabs-active a {color:#000000;}

/* Title line */

.title-line {
	margin: 2em 0 1.5em;
}
.title-line h2,
.title-line .filter {
	display: inline-block;
	vertical-align: baseline;
}
.title-line h2 {
	font-family: 'ProximaNova-Bold', Arial, sans-serif;
	font-size: 1.5em;
	margin: 0;
}
.title-line .filter {
	padding-left: 2.5em;
}

/* Modifications */
.title-line_centered h1,
.title-line_centered h2 {
	margin-left: 0;
	display: block;
	text-align: center;
}
.title-line_centered h1 {
	font-size: 2.25em;
	text-transform: uppercase;
	padding-bottom: .2em;
}
.title-line_centered h2 {
	font: italic 0.9375em Georgia, serif;
	margin-bottom: 2em;
}
.title-line_services {
	margin-top: 1em;
}

/* /Title line */

/* Filter dropdown */

.filter {
	position: relative;
}
.filter_lnk {
	font-size: .9375em;
	font-family: 'ProximaNova-Regular', Arial, sans-serif;
}
.filter_lnk:after {
	content: '';
	display: inline-block;
	vertical-align: middle;
	margin-left: 10px;
	width: 5px;
	height: 3px;
	/*'../i/ard-sm.png'*/
	background: url('data:image/png;base64,')
	0 0 no-repeat;
}
.filter_dropbox {
	position: absolute;
	left: 0.625em;
	top: -1.25em;
	width: 30.625em;
	padding: 1.1em 1.6em;
	background: white;
	border: 1px solid #f2f2f2;
	border-bottom: none;
	box-shadow: 0 3px 1px rgba(0,0,0,.2);
	z-index: 100;
	font-size: 1em;
	font-family: 'ProximaNova-Regular', Arial, sans-serif;
	overflow: hidden;
	max-height: 20em;
	-webkit-transition: max-height .5s, opacity .5s, padding .5s;
	transition: max-height .5s, opacity .5s, padding .5s;
}
.filter_dropbox.hidden {
	display: block !important;
	visibility: visible !important;
	left: -9999px;
	max-height: 0;
	padding: 0;
	opacity: 0;
	-webkit-transition: max-height .5s, opacity .5s, padding .5s, left 0 .5s;
	transition: max-height .5s, opacity .5s, padding .5s, left 0 .5s;
}
/*mini-reset*/
.filter_dropbox dl,
.filter_dropbox dd,
.filter_dropbox dt,
.filter_dropbox ul,
.filter_dropbox li {
	margin: 0;
	padding: 0;
}
.filter_dropbox ul {
	list-style-type: none;
}
.filter_dropbox ul li,
.filter_all-link {
	font-size: 0.9375em;
	font-family: 'ProximaNova-Regular', Arial, sans-serif;
}
.filter_col1,
.filter_col2,
.filter_subcol1,
.filter_subcol2 {
	display: inline-block;
	vertical-align: top;
}
.filter_col1 {
	width: 12.5em;
}
.filter_col2 {
	width: 18.125em;
}
.filter_subcol1 {
	width: 7.5em;
}
.filter_dropbox dd {
	font-style: italic;
	opacity: .6;
	margin-bottom: .8em;
}
.filter_dropbox a {
	display: inline-block;
	height: 1.55em;
	-webkit-transition: color .2s, background .2s, box-shadow .2s;
	transition: color .2s, background .2s, box-shadow .2s;
}
a.filter_all-link {
	margin: 1.5em 0 1em;
}
.filter_dropbox a.active,
.filter_dropbox a:hover {
	background: #f5f5f5;
	box-shadow: -1em 0 0 .25em #f5f5f5, 1em 0 0 .25em #f5f5f5, 0 0 0 .25em #f5f5f5;
}
.filter_dropbox a.active {
	color: #8d8d8d;
	cursor: default;
	pointer-events: none;
}

/* /Filter dropdown */

/* Menu dropdown */

.menu {
	float: right;
}
.menu_lnk {
	display: block;
	width: 14px;
	height: 14px;
	/*'../i/menu-lines.png'*/
	background: url('data:image/png;base64,')
	0 0 no-repeat;
}
.text-version .menu_lnk {
	/*'../i/menu-blocks.png'*/
	background-image: url('data:image/png;base64,');
}

/* /Menu dropdown */

/* Works list */

.works-list {
	list-style-type: none;
	margin: -2% 0 0 -2%;
	padding: 0;
	clear: right;
}
ul.works-list li {
	float: left;
	padding: 0;
	font-size: 1em;
	width: 23%;
	margin: 2% 0 0 2%;
}
ul.works-list li.promo {
	width: 48%;
	margin-bottom: -2px;
}
.works-list .text-block {
	background: #f5f5f5;
	padding: 2.5em;
	width: 48%;
	min-height: 200px;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
.works-list .text-block h3 {
	margin-top: 0;
	font-family: 'ProximaNova-Bold';
	font-size: 1.25em;
	font: 1.25em/1.4 'ProximaNova-Bold', sans-serif;
	text-transform: uppercase;
}
.works-list .text-block h3:after {
	content: '';
	display: block;
	margin: 2.1875em 0;
	margin-left: 50%;
	position: relative;
	left: -1.9375em;
	height: 0.1875em;
	width: 3.875em;
	background: black;
}
.works-list .text-block p {
	font: 1.0625em/1.4 'Tahoma', sans-serif;
}
.works-list a {
	display: block;
	overflow: hidden;
}
.works-list img {
	width: 100%;
}
.works-list img {
	opacity: 1;
	-webkit-transition: opacity .5s, -webkit-transform .5s;
	transition: opacity .5s, transform .5s;
	-webkit-transform: scale(1.1);
	transform: scale(1.1);
}
.works-list img.loading {
	opacity: 0;
}
.works-list figure {
	position: relative;
}
.works-list figcaption {
	position: absolute;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	/*background: #ccc;*/
	background: rgba(0,0,0,0);
	-webkit-transition: background .5s;
	-moz-transition: background .5s;
	-ms-transition: background .5s;
	-o-transition: background .5s;
	transition: background .5s;
}
.works-list dl {
	color: white;
	margin: 0;
	position: absolute;
	bottom: -10em;
	left: 1.25em;
	right: 1.25em;
	-webkit-transition: bottom .5s;
	-moz-transition: bottom .5s;
	-ms-transition: bottom .5s;
	-o-transition: bottom .5s;
	transition: bottom .5s;
}
.works-list dd,
.works-list dt {
	margin: 0;
}
.works-list dd {
	font-family: 'ProximaNova-Bold', Arial, sans-serif;
	font-size: 1.125em;
}
.works-list dt {
	font-family: 'ProximaNova-Regular', Arial, sans-serif;
	font-size: .875em;
}
.works-list a:hover figcaption {
	background: url('../i/50perblack.png') 0 0 repeat;
}
.works-list a:hover img {
	-webkit-transform: scale(1);
	transform: scale(1);
}
.rgba .works-list a:hover figcaption {
	background: rgba(0,0,0,.5);
}
.works-list a:hover dl {
	bottom: 1.25em;
}

.text-version .works-list img {
	display: none;
}
.text-version .works-list figcaption {
	position: static;
}
.text-version .works-list a:hover figcaption {
	background: transparent;
}
.text-version .works-list dl {
	position: static;
}
.text-version .works-list dd,
.text-version .works-list dt {
	color: black;
	font-weight: normal;
	font-family: 'tahoma', sans-serif;
}
.text-version .works-list dd {
	font-size: 0.875em;
}
.text-version .works-list dt {
	font-size: 0.75em;
	color: #989898;
}
.text-version .works-list a {
	padding-left: 1.5em;
}
.text-version .works-list {
	position: relative;
	padding-top: 2em;
}
.text-version .works-list .text-block {
	position: relative;
	top: -2em;
	margin-top: 0;
}
.text-version .works-list:before {
	content: '';
	position: relative;
	left: 52%;
	top: -2em;
	margin-top: 2%;
	width: 48%;
	height: 2px;
	background: #e2e2e2;
}

/* /Works list */

/* Year line */

.year-line {
	margin: 3% 0;
	position: relative;
}
.year-line:before,
.year-line:after {
	content: '';
	position: absolute;
	top: 50%;
	height: 1px;
	background: #e2e2e2;
	z-index: -1;
}
.year-line:before {
	left: -2.95em;
	width: 2.95em;
}
.year-line:after {
	right: -2em;
	left: 0;
	height: 1px;
}
.year-line span {
	background: white;
	font-family: 'ProximaNova-Bold', Arial, sans-serif;
	font-size: 1em;
	padding: 0 .5em;
	margin-left: -.5em;
}

/* /Year line */

