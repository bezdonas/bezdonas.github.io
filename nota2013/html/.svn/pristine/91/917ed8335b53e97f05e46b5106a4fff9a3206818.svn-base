.circlewrapper .mask { 
	position: absolute;
	top: 0;
	left: 0; 
	margin:0;
	width: 100%; 
	height: 100%; 
	z-index: 1; 
	 }
.cw_70x70 .mask {background: url('../i/mask-round.png') no-repeat 0 50%;}

.all, .all:link, .all:visited {text-decoration:underline; display:block; margin-top:1.2em;}
.all, .all:hover, .all:active {text-decoration:none;}

.inner .more {
	position:absolute;
	top:-1.3em;
	left:14.3em;
	margin-top:0;
}
.button {
	padding:0 0.625em;
	height:2.6em;
	margin-top:1.25em;
	display:inline-block;
	vertical-align:top;
	font: 0.875em/2.6em 'ProximaNova-Semibold', Arial, sans-serif; 
	background:#ececec;
	outline: 0; 
	-moz-border-radius: 2px; -webkit-border-radius: 2px; border-radius: 2px;
}
.leftblock, .path {
	position:absolute;
	top:6.25em;
	left: 2.95em;
	width:7.5em;
}

.submenu {
	margin:0;
	padding:0;
	list-style-type:none;
	}
.content .submenu li {
	padding:0.22em 0;
	font: 0.75em/2.5em Tahoma, Arial, sans-serif;

	}
.submenu li a, .submenu li a:link, .submenu li a:visited {
	color:#acacac;
	}

.comment {
	padding:0.85em 0 0.9em 60px;
	position:relative;
	min-height:60px;
	border-bottom:1px solid #e2e2e2;
	}
.comment a {
	display:block;
	color:#3b5e99;
	font:bold 0.75em/1.75em Tahoma, Arial, sans-serif;
	}
.comment a img {
	display:block;
	position:absolute;
	top:1.6em;
	left:0;
	max-width:60px;
	}
.comment p {font: 0.875em/1.4em Tahoma, Arial, sans-serif; margin-bottom:0;}
.comment .date {display:block; 	font: 0.75em/1.7em Tahoma, Arial, sans-serif;}


.content h1 { 	
	margin:0 0 0 -40px;
	font: 2.25em/1em 'ProximaNova-Bold', Arial, sans-serif; 
	padding-bottom:0.5em;
	position:relative;
	z-index:1;
}
.blog_post.imgby h1 {padding-bottom:0; max-height:1.5em;}

.tags {
	color:#acacac;
	display:inline-block;
	vertical-align:top;
	width:49%;
	margin-top:2em;
	}
.tags a {color:#acacac; font: 0.75em/1.875em Tahoma, Arial, sans-serif;}
.social {
	display:inline-block;
	vertical-align:top;
	width:50%;
	margin-top:2em;
	text-align:right;
	}
.social img {display:inline; margin:0;}

.pprev, .pnext {
	position: fixed;
	top:31em;
	display:block;
	width:17.5em;
	z-index:2;
	text-indent:-999em;
	padding:52px 0.125em 0 0.125em;
	font: 0.75em/1.15em Tahoma, Arial, sans-serif;

}
.pprev {left:0;}
.pprev:before {
		position:absolute;
		top:0;
		left:0;
		content:'';
		background:url('../i/icons.png') no-repeat 0 -140px;
		height:36px;
		width:36px;
}
.pnext {right:0;}
.pnext:before {
		position:absolute;
		top:0;
		right:0;
		content:'';
		background:url('../i/icons.png') no-repeat -40px -140px;
		height:36px;
		width:36px;
}
.no-touch .pprev:hover, .no-touch  .pnext:hover {text-indent:0;}
.no-touch .pprev:hover {text-align:right;}
.path {top:6.6em;}
.path a {
	display:inline-block;
	vertical-align:top;
	font: .875em/1.875em Tahoma, Arial, sans-serif; 
	padding-right:28px; 
	position:relative;
	}
.path a:after {
	position:absolute;
	top:0;
	right:0;
	content:'';
	background:url('../i/icons.png') no-repeat -180px 9px;
	height:21px;
	width:8px;
}
.blogcarousel {
	margin: 1em auto;
	width: 58.75em;
	width: 940px;
/*
	height:700px;
*/
	position: relative;
	}
.blogcarousel div {
	margin: 0 auto;
	padding: 0;
	position: relative;
	width: 940px;
}
.blogcarousel .pic {
	margin: 0 auto;
	padding: 0;
	position: absolute;
	top: 0;
	left: 0;
	z-index: 0;
}
.blogcarousel .pic img, .slideoverlay .pic img {
	display: block;
	position: absolute;
	top: 50%;
	left: 50%;
}
.slideoverlay .pic img {
	opacity: 1;
}
.blogcarousel .caption {
	font: 0.875em/1.15em Tahoma, Arial, sans-serif;
	color: #959595;
	padding: 1.5em 0 0 2.25em;
	width: 650px;
	position: absolute;
	top: 100%;
	left: 0;
}
.slideoverlay .pic .caption { display: none; }
.blogcarousel .statblock {
	display: inline-block;
	vertical-align: top;
	width: 15em;
	padding-top: 16px;
	position: absolute;
	top: 100%;
	right: 0;
	text-align: right;
}
.blogcarousel .stat {
	position:relative; 
	display:inline-block;
	vertical-align:middle;
	text-align:center;
	width:4.6875em;
	margin-top:-1.2em;
	font: 1.2em/1.15em Tahoma, Arial, sans-serif; 
	color:#000000;
	margin:0 1.875em;
	}
.blogcarousel .pic:before {
	position: absolute;
	bottom: 0;
	right: 6em;
	content: '';
	z-index: 2;
	margin-left: -6px;
	width: 1px; height: 1px; 
	border-top: solid 5px rgba(254,254,254,0);
	border-right: solid 5px rgba(254,254,254,0);
	border-bottom: solid 6px #ffffff; 
	border-left: solid 5px rgba(254,254,254,0); 
}
.no-rgba .blogcarousel .stat:before {height:8px; width:12px; background: url('../i/pip.png') no-repeat 0 0; }
.blogcarousel .previmg, .blogcarousel .nextimg {
	display:inline-block;
	vertical-align:middle;
	outline:none;
	height: 20px;
	width: 20px;
	z-index:1;
	background: url('../i/icons.png') no-repeat -220px 0; 
}
.blogcarousel .previmg {background-position:-260px 0; }
.blogcarousel .previmg.disable {background-position:-200px 0;}
.blogcarousel .nextimg.disable {background-position:-240px 0;}

.slideoverlay { position: absolute; top: 0; left: 0; width: 100%; height: 100%; z-index: 9999; overflow: hidden; }

#map1 {position:relative; min-height:44.625em; width:100%; margin-top:-2.6em; height:100%;}
#map2 {position:relative; min-height:44.625em; width:100%; margin-top:-2.6em; height:100%;}

.filtrOffice {
	position:absolute;
	top:5.8em;
	left:17.5em;
	z-index:1;
	}
.filtrMap {
	position:absolute;
	bottom:5em;
	right:0.625em;
	}
.filtrMap a {
	display:table-cell;
	vertical-align:top;
	font: 0.75em/1.875em Tahoma, Arial, sans-serif; 
	padding:0.685em;
