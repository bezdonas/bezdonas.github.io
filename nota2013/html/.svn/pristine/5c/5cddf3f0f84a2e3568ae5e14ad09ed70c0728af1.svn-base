			<!-- YOUR_API_KEY получить здесь: https://developers.google.com/maps/documentation/javascript/tutorial?hl=ru#api_key -->
			<div id="map1"></div>
			<div id="map2"></div>
			<script type="text/javascript">
				$(window).load(function() {
					initialize();
				});
				function initialize() {
							var mapOptions = {
								panControl: false,
								zoomControl: false,
								mapTypeControl: false,
								scaleControl: false,
								draggable: false,
								streetViewControl: false,
								overviewMapControl: false,
								styles: [
								  {
									stylers: [
									  { saturation: -100 }
									]
								  }
								],
								center: new google.maps.LatLng(55.81168,37.635665),
								zoom: 16,
								maxZoom: 16,
								minZoom: 16,
								mapTypeId: google.maps.MapTypeId.ROADMAP
							};
							var map = new google.maps.Map(document.getElementById('map1'), mapOptions);
							var marker = new google.maps.Marker({ // настраиваем маркер и позиционируем его на карту
								position: new google.maps.LatLng(55.81168,37.635665),
								map: map,
								icon: './i/buble.png'
							});
							var marker2 = new google.maps.Marker({ // настраиваем маркер и позиционируем его на карту
								position: new google.maps.LatLng(45.023877,38.970157),
								map: map,
								icon: './i/buble.png'
							});

							map.setZoom(16); // приблизить
							$('.ui-tabs-nav a').click(function(){
						if($(this).parent().is(':first-child'))
							{
								setTimeout( function(){
										map.setCenter(marker.position); // центрировать на маркер
									},0);
							}
							else {
								setTimeout( function(){
										map.setCenter(marker2.position); // центрировать на маркер
									},0);
							}
									return false;
					});
				};
			</script>
			<script type="text/javascript">
				ymaps.ready(init);
				var myMap, myPlacemark, myPlacemark2;
				function init(){     
					myMap = new ymaps.Map ('map2', {
						center: [55.81168,37.635665],
						zoom: 16,
					});
				myPlacemark = new ymaps.Placemark([55.81168,37.635665], { content: 'NOTAMEDIA', balloonContent: '129085, г. Москва, Проспект Мира, д. 101' }, {
						iconImageHref: './i/buble.png',
						iconImageSize: [71, 80],
						iconImageOffset: [-35, -40]
					});
				myPlacemark2 = new ymaps.Placemark([45.023877,38.970157], { content: 'NOTAMEDIA', balloonContent: '129085, г. Краснодар, Проспект Мира, д. 101' }, {
						iconImageHref: './i/buble.png',
						iconImageSize: [71, 80],
						iconImageOffset: [-35, -40]
					});

				myMap.geoObjects.add(myPlacemark);
				myMap.geoObjects.add(myPlacemark2);
				$('.ui-tabs-nav a').click(function(){
						if($(this).parent().is(':first-child'))
							{
								setTimeout( function(){
										myMap.panTo([55.81168,37.635665], {
											flying: false,
											delay: 0,
											duration: 500
										});
									},0);
							}
							else {
								setTimeout( function(){
										myMap.panTo([45.023877,38.970157], {
											flying: false,
											delay: 0,
											duration: 500
										});
									},0);
							}
									return false;
					});
					};

			</script>


