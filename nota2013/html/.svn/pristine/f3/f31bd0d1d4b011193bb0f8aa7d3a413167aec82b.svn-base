	color:#000000;
	margin:0 1.875em;
	}
.blogcarousel .pic:before {
	position: absolute;
	bottom: 0;
	right: 6em;
	content: '';
	z-index: 2;
	margin-left: -6px;
	width: 1px; height: 1px; 
	border-top: solid 5px rgba(254,254,254,0);
	border-right: solid 5px rgba(254,254,254,0);
	border-bottom: solid 6px #ffffff; 
	border-left: solid 5px rgba(254,254,254,0); 
}
.no-rgba .blogcarousel .stat:before {height:8px; width:12px; background: url('../i/pip.png') no-repeat 0 0; }
