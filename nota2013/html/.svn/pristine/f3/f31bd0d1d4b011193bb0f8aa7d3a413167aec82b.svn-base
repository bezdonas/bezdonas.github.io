	outline:none;
	height: 20px;
	width: 20px;
	z-index:1;
	background: url('../i/icons.png') no-repeat -220px 0; 
}
.blogcarousel .previmg {background-position:-260px 0; }
.blogcarousel .previmg.disable {background-position:-200px 0;}
.blogcarousel .nextimg.disable {background-position:-240px 0;}

.slideoverlay { position: absolute; top: 0; left: 0; width: 100%; height: 100%; z-index: 9999; overflow: hidden; }

