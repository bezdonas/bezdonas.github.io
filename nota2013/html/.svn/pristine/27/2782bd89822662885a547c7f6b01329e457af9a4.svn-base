						<script type="text/javascript">
								/*
								  FS Studio
								*/
								 

								(function($) {


								  var $block = $('div.lentaWorks');

								  var $view = 1;
								  var $next = $block.find('div.next');
								  var $prev = $block.find('div.prev');

								  var $lent = $block.find('div.lenta');

								  var $blocksView    = $lent.find('img');
								  var $blocksPreview = $block.find('div.preview > div');

								  var cur = 1;
								  var qty = $blocksPreview.length;


								  $next.removeClass('hide');
								  $prev.removeClass('hide');
								  
								  $blocksView.not(':eq(0)').fadeOut();
								  $blocksView.css({ position: 'absolute', top: 0, left: 0 });
								  $blocksPreview.css({cursor : 'pointer'});


								  var t=$('.preview div span:first');
								  var $prv = $('div.preview');
								  var $frame = $('<span>').css({position:'absolute', left: 210, width: 216, height: 164,border:'6px solid #92949b', cursor:'pointer'});

								  var start = 210;
								  var d = 262;


								  var curCss;
								  var visitedCss;

								  var isFsstudio;
								  var isMascotte;


								  if( $('div.screen.lentaWorks.mascotte').length == 1 ){ // bugaga
									isMascotte = true;
									isFsstudio = false;
									
									curCss = {opacity: .5};
									visitedCss = {opacity: 1};

								  } else {
									isMascotte = false;
									isFsstudio = true;

									visitedCss = {opacity: 1};
									curCss = {opacity: .5};
									//$blocksPreview.eq(0).addClass('_visited').css( curCss );
								  }

								  if( ! $.browser.msie )
									$frame.appendTo( $prv );

								  $blocksPreview.eq(0).addClass('_visited').find('img').css( curCss );

								  // Refresh view
								  function RefreshView(){
									$blocksView.not(':eq('+(cur-1)+')').fadeOut( 333 );
									$blocksView.eq(cur-1).fadeIn( 333 );

									$blocksPreview.eq(cur-1).siblings().removeClass('cur');
									$blocksPreview.filter('._visited').find('img').css( visitedCss );

									if( ! $.browser.msie ){
									  $frame.stop().animate({left: start+d*(cur-1)}, 333, function(){
										$blocksPreview.eq(cur-1).addClass('cur _visited').find('img').css( curCss );
									  });
									} else {
									  $blocksPreview.eq(cur-1).addClass('cur _visited').find('img').css( curCss );
									}
								  }


								  $blocksPreview.each(function(i){ $blocksPreview.eq(i).click( function(){
									cur=i+1;
									RefreshView();
								  })
								  });
								  
								  // next
								  $next.click( function(ev){

									cur++;

									if( cur > qty )
									  cur = 1;

									RefreshView();
								  });

								  // prev
								  $prev.click( function(ev){
									
									cur--;
								  
									if( cur < 1 )
									  cur = qty;

									RefreshView();

								  });

								})(jQuery);

						</script>
